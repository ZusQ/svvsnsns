local HttpService = game:GetService("HttpService")
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

local HWID = gethwid and gethwid() or "Unknown" -- L·∫•y HWID
local KeyFileURL = "https://raw.githubusercontent.com/Phatdepzaicrystal/Key/main/keys.json" -- üî• Link ch·ª©a key
local DiscordWebhook = "https://discord.com/api/webhooks/1354018477828210788/2VNzzC5fY4zFB291hfDjfLAip541-zcNxF-H0IJNAp6a6nUBf9mKxu6TnjRPSPPrG4o9" -- üî• Webhook Discord

-- T·∫£i danh s√°ch key t·ª´ GitHub
local function GetKeys()
    local success, response = pcall(function()
        return HttpService:GetAsync(KeyFileURL)
    end)

    if success then
        local keysData = HttpService:JSONDecode(response)
        return keysData
    else
        warn("‚ö†Ô∏è L·ªói t·∫£i danh s√°ch key t·ª´ GitHub!")
        return nil
    end
end

-- Ki·ªÉm tra xem HWID ƒë√£ t·ªìn t·∫°i trong danh s√°ch key ch∆∞a
local function CheckKey()
    local keys = GetKeys()
    if not keys then return false end

    for _, entry in pairs(keys) do
        if entry.HWID == HWID then
            print("‚úÖ Key h·ª£p l·ªá! Ti·∫øn h√†nh g·ª≠i HWID l√™n webhook...")
            return entry.Key -- Tr·∫£ v·ªÅ Key n·∫øu t√¨m th·∫•y HWID
        end
    end
    return nil
end

-- G·ª≠i th√¥ng tin HWID l√™n Discord Webhook
local function SendToWebhook(verifiedKey)
    local data = {
        content = "**:key: Y√™u c·∫ßu Redeem Key**\n",
        embeds = {{
            title = "Th√¥ng tin ng∆∞·ªùi d√πng",
            fields = {
                { name = ":small_blue_diamond: HWID", value = HWID, inline = true },
                { name = ":id: User ID", value = tostring(LocalPlayer.UserId), inline = true },
                { name = ":bust_in_silhouette: Username", value = LocalPlayer.Name, inline = true },
                { name = ":key: Key ƒê∆∞·ª£c Duy·ªát", value = verifiedKey, inline = true }
            },
            color = 16711680
        }}
    }

    local request = syn and syn.request or http_request or request
    if request then
        request({
            Url = DiscordWebhook,
            Method = "POST",
            Headers = { ["Content-Type"] = "application/json" },
            Body = HttpService:JSONEncode(data)
        })
    end
end

-- Ch·∫°y script n·∫øu key h·ª£p l·ªá
local verifiedKey = CheckKey()
if verifiedKey then
    SendToWebhook(verifiedKey)
    print("‚úÖ Key h·ª£p l·ªá! Ch·∫°y script...")

    -- üî• Ch·∫°y script
    getgenv().Team = "Marines"
loadstring(game:HttpGet("https://raw.githubusercontent.com/Dex-Bear/VxezeHubHopBoss/refs/heads/main/VxezeHubHopDough.txt"))()
else
    print("‚ùå Key kh√¥ng h·ª£p l·ªá ho·∫∑c HWID ch∆∞a ƒë∆∞·ª£c ƒëƒÉng k√Ω!")
end
